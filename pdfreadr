    "def download_file(pdf):\n",
    "  tablature = pdf.split('/')[4]\n",
    "  with requests.get(pdf) as req:\n",
    "    with open(tablature, 'wb') as file:\n",
    "      file.write(req.content)\n",
    "  return tablature"
